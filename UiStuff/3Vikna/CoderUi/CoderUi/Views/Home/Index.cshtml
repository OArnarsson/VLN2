@model IEnumerable<CoderUi.Assignment>
@{
    ViewBag.Title = "Home Page"; 
}



    <div class="main">
        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-lg-3">
                    <h2>@item.name</h2>
                    <h6>@item.courseName</h6>
                    <h4>@item.status</h4>
                    <h5>Value: @item.value %</h5>
                    <h5>Start: @item.start</h5>
                    <h5>End: @item.end</h5>
                </div>
            }
        <div/>
    </div>